main	main.cpp	/^int main()$/;"	f	signature:()
operator ()	main.cpp	/^                 size_t operator()( const string& str) const  \/\/模板特化必须加const两个$/;"	f	class:__gnu_cxx::hash	access:public	signature:( const string& str) const
operator ()	main.cpp	/^            bool operator()(Key oldKey,Key newKey)$/;"	f	class:cmpFunctor	access:public	signature:(Key oldKey,Key newKey)
operator <<	main.cpp	/^    ostream& operator<<(ostream& out,pair<T1,T2> mappair)\/\/<<输出运算符重载,必须作为非成员函数$/;"	f	signature:(ostream& out,pair<T1,T2> mappair)
