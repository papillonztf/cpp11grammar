__gnu_cxx::hash	main.cpp	/^        class hash<string>$/;"	c	namespace:__gnu_cxx	file:
__gnu_cxx::hash::operator ()	main.cpp	/^                 size_t operator()( const string& str) const  \/\/模板特化必须加const两个$/;"	f	class:__gnu_cxx::hash	access:public	signature:( const string& str) const
cmpFunctor	main.cpp	/^    class cmpFunctor$/;"	c	file:
cmpFunctor::operator ()	main.cpp	/^            bool operator()(Key oldKey,Key newKey)$/;"	f	class:cmpFunctor	access:public	signature:(Key oldKey,Key newKey)
